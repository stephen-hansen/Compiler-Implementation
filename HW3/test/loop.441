class LISTNODE [
   fields val:int, next:LISTNODE, isnull:int
   method getNext() returning LISTNODE with locals:
      return &this.next
   method getVal() returning int with locals:
      return &this.val
   method isNull() returning int with locals:
      return &this.isnull
]
class STACK [
   fields list:LISTNODE
   method push(v:int) returning int with locals tmp:LISTNODE:
      tmp = @LISTNODE
      !tmp.val = v
      !tmp.next = &this.list
      !tmp.isnull = 0
      !this.list = tmp
      return 0
   method pop() returning int with locals tmp:int:
      if ^&this.list.isNull(): {
         return 0
      } else {
         tmp = ^&this.list.getVal()
         !this.list = ^&this.list.getNext()
         return tmp
      }
]
class STACKER [
   fields
   method do(stk:STACK) returning int with locals x:int, v:int:
      x = 20
      while x: {
         _ = ^stk.push(x)
         x = (x - 1)
      }
      v = ^stk.pop()
      while v: {
         print(v)
         v = ^stk.pop()
      }
]
main with node:LISTNODE, stk:STACK, stkr:STACKER:
   stk = @STACK
   node = @LISTNODE
   !node.isnull = 1
   !stk.list = node
   stkr = @STACKER
   _ = ^stkr.do(stk)

