class FOO [
   fields
   method bar(a:int) returning int with locals b:int, c:int, d:int, e:int, f:int, g:int, h:int, j:int, k:int:
      b = a
      c = 5
      d = (b + c)
      e = a
      f = 6
      g = (e + f)
      h = a
      j = 7
      k = (h + j)
      return 0
]

main with i:int, f:FOO:
   f = @FOO
   i = 5
   _ = ^f.bar(i)
   
