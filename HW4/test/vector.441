class VECTOR [
   fields a0:int, a1:int, a2:int, a3:int
   method add(other:VECTOR) returning int with locals i:int:
      !this.a0 = (&this.a0 + &other.a0)
      !this.a1 = (&this.a1 + &other.a1)
      !this.a2 = (&this.a2 + &other.a2)
      !this.a3 = (&this.a3 + &other.a3)
      return 0
]

main with v:VECTOR, u:VECTOR:
   v = @VECTOR
   u = @VECTOR
   _ = ^v.add(u)
   
